<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GuestBookRazorPages</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/GuestBookRazorPages.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/animationFon.css"/>
    <link rel="stylesheet" href="~/css/indexMenu.css"/>
    <link rel="stylesheet" href="~/css/RegistrationMenu.css"/>
    <link rel="stylesheet" href="~/css/feedBackPage.css"/>



</head>
<body>
    
    <div class="container">
        <main role="main" class="pb-3">
            <div id="particles-js">
            @RenderBody()
            </div>
        </main>
    </div>

 
    <script src="~/js/particles.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.jsx" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>